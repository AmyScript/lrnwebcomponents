  },
  "devDependencies": {
    "@polymer/iron-demo-helpers": "^3.0.1",
    "@webcomponents/webcomponentsjs": "^2.1.1",
    "concurrently": "^4.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify-es": "^1.0.4",
    "lodash": "^4.17.10",
    "polymer-build": "^3.0.4",
    "polymer-cli": "^1.7.1",
    "uglify-es": "^3.3.9",
    "wct-browser-legacy": "^1.0.1"
  },
