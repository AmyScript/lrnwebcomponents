{
  "name": "@lrnwebcomponents/lrn-button",
  "wcfactory": {
    "className": "LrnButton",
    "customElementClass": "PolymerElement",
    "elementName": "lrn-button",
    "generator-rhelement-version": "0.0.8"
  },
  "version": "0.0.4",
  "description": "Button element for PolymerElement",
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "lrn-button.umd.js",
  "module": "lrn-button.es5.js",
  "es2015": "lrn-button.js",
  "amd": "lrn-button.amd.js",
  "scripts": {
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "open ./src && concurrently --kill-others 'npm run watch' 'npm run serve'",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@rhelements/<%= elementName %>/test/",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
