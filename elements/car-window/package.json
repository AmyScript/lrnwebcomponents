{
  "name": "@lrnwebcomponents/car-window",
  "wcfactory": {
    "className": "CarWindow",
    "customElementClass": "SkateJS",
    "elementName": "car-window",
    "generator-wcfactory-version": "0.3.1",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/car-window.css",
      "scss": "src/car-window.scss",
      "html": "src/car-window.html",
      "js": "src/car-window.js",
      "properties": "src/car-window-properties.json",
      "hax": "src/car-window-hax.json"
    }
  },
  "version": "0.0.22",
  "description": "Describe how a car window works",
  "repository": {
    "type": "git",
    "url": "git@github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "car-window.js",
  "module": "car-window.es6.js",
  "umd": "car-window.umd.js",
  "amd": "car-window.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/car-window/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "open ./src && concurrently --kill-others 'yarn run watch' 'yarn run serve'",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@skatejs/renderer-lit-html": "^0.2.2",
    "lit-html": "^0.12.0",
    "skatejs": "^5.2.4"
  },
  "devDependencies": {
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.0.2",
    "@webcomponents/webcomponentsjs": "2.1.3",
    "concurrently": "4.0.1",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.4",
    "lodash": "4.17.11",
    "polymer-build": "3.0.4",
    "polymer-cli": "1.8.0",
    "wct-browser-legacy": "1.0.1"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
