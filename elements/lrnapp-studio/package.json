{
  "name": "@lrnwebcomponents/lrnapp-studio",
  "wcfactory": {
    "className": "LrnappStudio",
    "customElementClass": "PolymerElement",
    "elementName": "lrnapp-studio",
    "generator-wcfactory-version": "0.3.1",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/lrnapp-studio.css",
      "scss": "src/lrnapp-studio.scss",
      "html": "src/lrnapp-studio.html",
      "js": "src/lrnapp-studio.js",
      "properties": "src/lrnapp-studio-properties.json",
      "hax": "src/lrnapp-studio-hax.json"
    }
  },
  "version": "0.0.29",
  "description": "Automated conversion of lrnapp-studio/",
  "repository": {
    "type": "git",
    "url": "git@github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "lrnapp-studio.js",
  "module": "lrnapp-studio.es6.js",
  "umd": "lrnapp-studio.umd.js",
  "amd": "lrnapp-studio.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/lrnapp-studio/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "open ./src && concurrently --kill-others 'yarn run watch' 'yarn run serve'",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
